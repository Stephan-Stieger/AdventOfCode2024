   "source": [
    "# Read the file and store the data in a list\n",
    "with open('data.txt', 'r') as file:\n",
    "    File = file.readlines()\n",
    "\n",
    "data = \"\"\n",
    "for line in File:\n",
    "    data += line\n",
    "\n",
    "# Print the data to verify\n",
    "print(data)"
